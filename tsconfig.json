{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true,
        "module": "ES6",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "outDir": "./dist",
        "rootDir": "./src"
    },
    "exclude": ["node_modules"]
}
